@model IList<OnlineStore.Model.ViewModel.BannerViewModel>
<div id="homepage-slider">
    <ul id="homeslider">
        @foreach (var item in Model)
        {
            <li class="homeslider-container">
                <a href="#" title="@item.Name">
                    <img src="@(item.ImagePath)" width="1770" height="721" alt="" />
                </a>
            </li>
        }
    </ul>
    <div id="bx-pager-thumb">
        @foreach (var item in Model)
        {
            <a class="thumb-1" data-slide-index="0" href="">
                <img src="@(item.ImagePath)" alt="">
            </a>
        }
    </div>
</div>